{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": [
      "error",
      "double"
    ],
    "indentation": [
      "error",
      4
    ],
    "visibility-first": [
      "error"
    ],
    "function-whitespace": [
      "error"
    ],
    "operator-whitespace": [
      "error"
    ]
  }
}
